{
  "singleQuote": true,
  "trailingComma": "es5",
  "tabWidth": 2,
  "importOrder": [
    "^react(.*)",
    "<THIRD_PARTY_MODULES>",
    "interface",
    "config",
    "hooks/",
    "(components/|./index)",
    "^../(.*)$",
    "/styles,"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
